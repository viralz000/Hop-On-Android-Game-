<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/newbackground">

    <ImageView
        android:id="@+id/manilacity"
        android:layout_width="match_parent"
        android:layout_height="400dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="-2dp"
        android:layout_marginTop="140dp"
        android:layout_marginEnd="-1dp"
        android:background="@drawable/manila" />

    <ImageButton
        android:layout_width="170dp"
        android:layout_height="145dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="250dp"
        android:background="@drawable/level13"
        android:id="@+id/level13"/>

    <ImageButton
        android:layout_width="170dp"
        android:layout_height="145dp"
        android:layout_marginTop="250dp"
        android:layout_marginLeft="200dp"
        android:id="@+id/level14"
        android:background="@drawable/level14"/>

    <ImageButton
        android:layout_width="170dp"
        android:layout_height="145dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="350dp"
        android:background="@drawable/level15"
        android:id="@+id/level15"/>

    <ImageButton
        android:layout_width="170dp"
        android:layout_height="145dp"
        android:layout_marginTop="350dp"
        android:layout_marginLeft="200dp"
        android:background="@drawable/level16"
        android:id="@+id/level16"/>

    <ImageButton
        android:id="@+id/previousbutton4"
        android:layout_width="120dp"
        android:layout_height="130dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="450dp"
        android:background="@drawable/previousbutton" />

    <ImageButton
        android:id="@+id/nextbutton"
        android:layout_width="150dp"
        android:layout_height="130dp"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="260dp"
        android:layout_marginTop="450dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/nextbutton" />

    <ImageButton
        android:layout_width="175dp"
        android:layout_height="175dp"
        android:layout_marginTop="440dp"
        android:layout_marginLeft="120dp"
        android:background="@drawable/fourof4"
        android:id="@+id/fourof4"/>

</RelativeLayout>